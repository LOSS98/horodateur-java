<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="fr.insa.horodateurjava.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <!-- TableView pour afficher les places -->
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="colNumero" text="Numéro" />
            <TableColumn fx:id="colDisponibilite" text="Disponibilité" />
            <TableColumn fx:id="colEnTravaux" text="En travaux" />
            <TableColumn fx:id="colEtage" text="Étage" />
            <TableColumn fx:id="colTarifHoraire" text="Tarif horaire" />
        </columns>
    </TableView>

</VBox>
